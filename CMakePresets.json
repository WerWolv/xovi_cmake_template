{
  "version": 3,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 20,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "base",
      "displayName": "Base",
      "description": "Base configuration for all builds",
      "hidden": true,
      "binaryDir": "${sourceDir}/build/${presetName}",
